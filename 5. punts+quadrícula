float inicio1=0.0;
float fin1=PI*2;
float inicio2=0.63;
float fin2=5.61;
float temp=0.0;
int x=330;
int y=150;


void setup (){
  size (840, 600);
  strokeWeight (3);
  smooth ();
  frameRate(7);

}
 
void draw (){
 background (0); 
    for (int i=0; i<height; i= i +60) {
  stroke(255); 
  line(0, i, width, i); 
} 
 for (int i=0; i<width; i= i +60) { 
  stroke(255); 
  line(i, 0, i, height); 
} 

 noStroke();
 fill(225,246,8);
 arc (x,y, 50, 50, inicio1, fin1);
 fill(0);
 ellipse(x-9,y-9,10,10);
 temp=inicio1;
 inicio1=inicio2;
 inicio2=temp;
 temp=fin1;
 fin1=fin2;
 fin2=temp;
 fill(255);
 for (int i=30; i<height; i= i +60) { 
 for (int j=30; j<width; j= j +60) {
  stroke(255); 
  ellipse(j, i, 5, 5); 
} 
} 

}

void keyPressed() {
 if (key=='a') {
   x=x-60;
 }
  if (key=='d') {
   x=x+60;
 }
  if (key=='w') {
   y=y-60;
 }
  if (key=='s') {
   y=y+60;
 }
}
